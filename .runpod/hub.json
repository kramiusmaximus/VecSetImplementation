{
  "title": "VecSetEdit",
  "description": "Serverless 3D mesh editing with VecSet representation and image-guided editing.",
  "type": "serverless",
  "category": "image",
  "config": {
    "runsOn": "GPU",
    "endpointType": "LB",
    "containerDiskInGb": 120,
    "gpuCount": 1,
    "gpuIds": "AMPERE_80,ADA_24,AMPERE_48",
    "allowedCudaVersions": [
      "12.8",
      "12.7",
      "12.6",
      "12.5",
      "12.4",
      "12.3",
      "12.2",
      "12.1",
      "12.0"
    ],
    "env": [
      {
        "key": "HF_HOME",
        "value": "/workspace/.cache/huggingface"
      },
      {
        "key": "TRANSFORMERS_CACHE",
        "value": "/workspace/.cache/huggingface"
      }
    ]
  }
}
